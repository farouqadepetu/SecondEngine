.PHONY: clean All

All:
	@echo "----------Building project:[ Client_Datagram - Debug ]----------"
	@cd "Client_Datagram" && "$(MAKE)" -f  "Client_Datagram.mk"
clean:
	@echo "----------Cleaning project:[ Client_Datagram - Debug ]----------"
	@cd "Client_Datagram" && "$(MAKE)" -f  "Client_Datagram.mk" clean
